[Default Applications]

# Settings used by xdg-open will to determine how to open filetypes.
# All .desktop entries can also be seen and changed in:
# /usr/local/share/applications
# ~/.local/share/applications

image/=swayimg.desktop
image/jpg=swayimg.desktop
image/jpeg=swayimg.desktop
image/png=swayimg.desktop
image/bmp=swayimg.desktop
image/gif=swayimg.desktop
image/webp=swayimg.desktop
image/svg+xml=org.inkscape.Inkscape.desktop

video/mp4=mpv.desktop
video/mkv=mpv.desktop

application/pdf=org.pwmt.zathura.desktop
application/postscript=org.pwmt.zathura.desktop
image/vnd.djvu=org.pwmt.zathura.desktop
application/vnd.comicbook-rar=org.pwmt.zathura.desktop
application/vnd.comicbook+zip=org.pwmt.zathura.desktop

text/=text_editor.desktop
text/plain=text_editor.desktop;
text/x-shellscript=text_editor.desktop;
text/html=firefox.desktop
text/htm=firefox.desktop

x-scheme-handler/magnet=torrent_client.desktop;
application/x-bittorrent=torrent_client.desktop;
